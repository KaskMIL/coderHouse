<body>

    <script>
        let num = prompt("ingrese un numero");
        let numInt = parseInt(num);
        if(numInt >= 0){
            alert("Ingresaste un numero positivo");
        }
        else(alert("Ingresaste un numero negativo"))
        if(numInt > 0 && numInt < 10){
            alert("tu numero es menor a 10");
        }
        else if(numInt > 10){
            alert("El numero ingresado es mayor a 10")
        }
    </script>
</body>
